# Composio Agent Gateway - Environment Variables Template
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# Composio API Configuration
COMPOSIO_API_KEY=your-composio-api-key-here
COMPOSIO_API_URL=https://api.composio.dev/api/v1

# Service Configuration
SERVICE_NAME=composio-agent
PORT=10000

# Feature Flags
KILL_SWITCH=false

# Logging
LOG_LEVEL=INFO

# Render-specific (automatically set by Render, no need to configure)
# PORT=10000
# PYTHON_VERSION=3.11.0
